                        <p>NekoStats</p>
                    </li>
                    <li class="statstext"></li>
                </ul>
                <ul class="playing">
                    <li class="maintexttop">
                        <p>internet stuff</p>
                    </li>
                    <li class="playingtext">webrings</li>
                </ul>
                <ul class="square">
                    <li class="maintexttop">
                        <p>sponsors</p>
                    </li>
                    <li class="squaretext"></li>
                </ul>
                <ul class="square">
                    <li class="maintexttop">
                        <p>not sponsors</p>
                    </li>
                    <li class="squaretext"></li>
                </ul>
                <ul class="follow">
                    <li class="followtext"></li>
                </ul>
            </ul>
        </div>
    </div>
</body>
